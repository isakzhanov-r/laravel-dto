{
    "name": "isakzhanov-r/laravel-dto",
    "description": "Data Transfer Object for VO",
    "license": "MIT",
    "authors": [
        {
            "name": "isakzhanov-r",
            "email": "isakzhanov.r@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/validation": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "isakzhanov-r/laravel-value-object": "^1",
        "mockery/mockery": "^0.9|^1.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0|^9.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "IsakzhanovR\\DataTransferObject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
